{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "lib": [
      "esnext",
      "dom",
      "es2019"
    ],
    "noEmit": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "incremental": true,
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules",
    "src-original"
  ]
}